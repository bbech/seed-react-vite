{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.5.0",
    "generators": {
      "publicApi": { 
        "generatorName": "typescript-axios",
        "glob": "./openapi/petapi.json",
        "output": "./generated-sources/publicApi"
      },
      "privateApi": { 
        "generatorName": "typescript-axios",
        "glob": "./openapi/petapi2.json",
        "output": "./generated-sources/privateApi"
      }
    }
  }
}
